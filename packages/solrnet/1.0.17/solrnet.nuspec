<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SolrNet</id>
    <version>1.0.17</version>
    <authors>Mauricio Scheffer and contributors</authors>
    <owners>Mauricio Scheffer and contributors</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://raw.githubusercontent.com/SolrNet/SolrNet/master/license.txt</licenseUrl>
    <projectUrl>https://github.com/solrnet/solrnet</projectUrl>
    <iconUrl>https://github.com/solrnet/solrnet/raw/master/Documentation/solr.png</iconUrl>
    <description>SolrNet is a .NET Open Source client for Apache Solr. This version of SolrNet is compatible with Solr 1.x to Solr 7.x. This package comes with a build in easy to use Dependency Injection for SolrNet.Core.</description>
    <repository url="https://github.com/solrnet/solrnet" />
    <dependencies>
      <group targetFramework=".NETFramework4.6">
        <dependency id="SolrNet.Core" version="1.0.17" exclude="Build,Analyzers" />
        <dependency id="CommonServiceLocator" version="2.0.2" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="SolrNet.Core" version="1.0.17" exclude="Build,Analyzers" />
        <dependency id="CommonServiceLocator" version="2.0.2" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>